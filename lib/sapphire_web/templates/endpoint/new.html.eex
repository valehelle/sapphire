<%= form_for @changeset, endpoint_path(@conn, :create, 1), fn f -> %>

  <%= text_input f, :name %>
  <%= error_tag f, :name %>

  <%= inputs_for f, :configs, fn c -> %>
    <%= text_input c, :url %>
    <%= error_tag c, :url %>
    <%= text_input c, :type %>
    <%= text_input c, :delay %>
    <%= textarea c, :body %>
  <% end %>
  
  <%= submit "Submit" %>
<% end %>      